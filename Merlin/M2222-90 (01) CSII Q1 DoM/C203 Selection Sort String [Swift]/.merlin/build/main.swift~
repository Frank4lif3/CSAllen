// Add your code below:

var array = unsortedStrings

func swap(array:inout[String], fI:Int, sI:Int) {
    let f = array[fI]
    let s = array[sI]
    
    array.remove(at:sI)
    array.insert(f, at:sI)
    array.remove(at:fI)
    array.insert(s, at:fI)
}

var swaps = 0
var tSwap = 0


for pass in 1 ..< array.count {
    var select = 0
    var index = pass
    repeat {
        if array[index] < array[select] {

        }
    } while index >= 1
    if select != pass {
        swap(array:&array,fI:pass,sI:select)
    }
}
