// Add your code below:
var array = unsortedStrings

func swap(array:inout[String], fI:Int, sI:Int) {
    let f = array[fI]

    array.remove(at:fI)
    array.insert(array[sI], at:fI)
    array.remove(at:sI)
    array.insert(f, at:sI)
}

